<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="vista.pantallaSeleccionModoController">
    <padding>
        <Insets top="50" right="50" bottom="50" left="50"/>
    </padding>

    <Button fx:id="nuevaPartidaBtn" text="Nueva Partida" onAction="#handleNuevaPartida"/>
    <Button fx:id="cargarPartidaBtn" text="Cargar Partida" onAction="#handleCargarPartida"/>

</VBox>
